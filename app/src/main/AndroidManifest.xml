        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme"
        android:debuggable="true"
        >

        <activity
            android:name="com.firstapplication.medianight.andoridhdm.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name="com.firstapplication.medianight.andoridhdm.BalanceActivity"
            android:label="@string/screenBalance" />

        <activity
            android:name="com.firstapplication.medianight.andoridhdm.DebtsCreditsActivity"
            android:label="@string/screenDebtsCredits" />

        <activity
            android:name="com.firstapplication.medianight.andoridhdm.DreamlistActivity"
            android:label="@string/screenDreamlist" />

        <activity
            android:name="com.firstapplication.medianight.andoridhdm.IncomeExpendAcitvity"
            android:label="@string/screenIncomeExpend" />


    </application>

</manifest>
